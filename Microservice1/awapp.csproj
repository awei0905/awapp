    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>6005f108-d10a-4ee3-aa10-f65c7054a4a1</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="6.0.3">
